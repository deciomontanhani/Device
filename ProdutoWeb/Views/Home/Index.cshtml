    <!-- Cabeçalho -->
    <header>
        <div class="container">
            <div class="intro-text">
                <div class="intro-heading"><br>Conheça a Viky:<br> sua assistente</div>
                <a href="#services" class="page-scroll btn btn-xl">Fale-me Mais</a>
            </div>
        </div>
    </header>
    <!-- Services Section -->
    <section id="services">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Serviços</h2>
                    <h3 class="section-subheading text-muted">Conheça as tarefas da sua nova assistente.</h3>
                </div>
            </div>
            <div class="row text-center">
                <div class="col-md-4">
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-home fa-stack-1x fa-inverse"></i>
                    </span>
                    <h4 class="service-heading">VikiCASA</h4>
                    <p class="text-muted">VikiCasa possui um sistema de controle da iluminação ambiente. Com uma pequena placa de Arduino instalada na sua casa, você economizará energia e terá total controle da sua residência.</p>
                </div>
                <div class="col-md-4">
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-heart fa-stack-1x fa-inverse"></i>
                    </span>
                    <h4 class="service-heading">VikiVIDA</h4>
                    <p class="text-muted"> VikiVida possui um sistema de localização que te informa qual a melhor rota para o trabalho, te avisa de locais que deve evitar e ainda te passa a previsão do tempo para que possa se planejar.</p>
                </div>
                <div class="col-md-4">
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-globe fa-stack-1x fa-inverse"></i>
                    </span>
                    <h4 class="service-heading">VIKINOTÍCIAS</h4>
                    <p class="text-muted">VikiNoticías te atualiza com as principais informações da sua cidade e também do mundo. Acompanhe a bolsa e a cotação da moeda diariamente através do aplicativo. </p>
                </div>
            </div>
        </div>
    </section>
    <!-- Sessão do portfólio-->
    <section id="projeto" class="bg-light-gray">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Notícias</h2>
                    <h3 class="section-subheading text-muted"></h3>
                </div>
            </div>



            <div class="row">
                @foreach (var item in Model.noticia)
                {



                    <div class="col-md-4 col-sm-6 portfolio-item">

                        <div class="portfolio-caption">
                            <h4>@Html.DisplayFor(m => item.Titulo)</h4>

                        </div>

                        <a href="#portfolioModal-@Html.DisplayFor(m => item.NoticiaId)" class="portfolio-link" data-toggle="modal">
                            <div class="portfolio-hover">
                                <div class="portfolio-hover-content">
                                    <i class="fa fa-plus fa-3x"></i>
                                </div>
                            </div>

                            <p>Clique aqui para saber mais</p>
                        </a>

                    </div>
                }





            </div>
        </div>



    </section>
    <!-- About Section -->
    <section id="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Sobre a Viky </h2>
                    <h3 class="section-subheading text-muted">A assistente certa para o seu lar.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <ul class="timeline">
                        <li>
                            <div class="timeline-image">
								<span class="fa-stack fa-4x">
                                <i class="fa fa-circle fa-stack-2x text-primary"></i>
                                <i class="fa fa-eye fa-stack-2x center "></i>
                                </span>
							</div>
              <div class="timeline-panel">
                                <div class="timeline-heading">
                                  <h4 class="subheading">A Viki cuida da sua casa</h4>


                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">A assistente Viki te informa em tempo real todas as movimentações que estão ocorrendo na sua casa. Deixou o seu cão sozinho? 
                                    Não esquenta, a Vicky está de olho pra você!</p>

                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                 <span class="fa-stack fa-4x">
                                <i class="fa fa-circle fa-stack-2x text-primary"></i>
                                <i class="fa fa-plane fa-stack-2x center "></i>
                                </span>

                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    
                                    <h4 class="subheading">Vai viajar? Peça ajuda para sua assistente!</h4>


                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted"> Se você está planejando aquela viagem bacana, a Viki pode te ajudar! A sua assistente vai te atualizar diariamente com boletins informativos para que você  acompanhe a cotação da moeda de seu destino. </p>

                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">

                            <div class="timeline-image">
                                 <span class="fa-stack fa-4x">
                                 <i class="fa fa-circle fa-stack-2x text-primary"></i>
                                 <i class="fa fa-paperclip fa-stack-2x center "></i>
                                </span>

                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    
                                    <h4 class="subheading">Não está esquecendo de nada?</h4>


                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">"E aí, será que hoje vai chover?". "Levo ou não levo um casaco?".  Com a sua assistente do lar você não terá mais esse tipo de dúvida.</p>

                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <span class="fa-stack fa-4x">
                                <i class="fa fa-circle fa-stack-2x text-primary"></i>
                                <i class="fa fa-usd fa-stack-2x center "></i>
                                </span>

                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    
                                    <h4 class="subheading">Economize com a ajuda da sua assistente </h4>


                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">Mais que uma assistente, uma verdadeira economia! Com a Viki, você controla a iluminação da sua casa. Controle a luz ambiente, ilumine corredores, apague para economizar. </p>

                                </div>
                            </div>
                        </li>
                        <li class="timeline-inverted">
                            <div class="timeline-image">
                                <h4>Contrate a
                                    <br>Viki para <br>
                                         sua casa!<br></h4>





                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>


    <!-- Área de contato -->
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Clientes</h2>
                    <h3 class="section-subheading text-muted">Gostou da nossa assistente Viky?.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    
                   
                    
                    @using (Html.BeginForm("CadastrarCliente", "Home"))
                    {

                        

                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    @Html.TextBoxFor(model => model.cliente.Nome, new { placeholder = "Seu Nome *", @class = "form-control", id = "name" })
                                    <!--<input type="text" class="form-control" placeholder="Seu nome *" id="name" required data-validation-required-message="Nome">-->
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">

                                    @Html.TextBoxFor(model => model.cliente.Sobrenome, new { placeholder = "Seu Sobrenome *", @class = "form-control", id = "sobrenome" })
                                    <!--<input type="text" class="form-control" placeholder="Seu sobrenome *" id="sobrenome" required data-validation-required-message="Sobrenome.">-->
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    @Html.TextBoxFor(model => model.cliente.Email, new { placeholder = "Seu Email *", @class = "form-control", id = "email" })
                                    <!--<input type="email" class="form-control" placeholder="Seu Email *" id="email" required data-validation-required-message="Email-->
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    @Html.TextBoxFor(model => model.cliente.CPF, new { placeholder = "Seu CPF *", @class = "form-control", id = "CPF", maxLength = "15" })
                                    <!--<input type="text" class="form-control" placeholder="Seu CPF *" id="CPF" required data-validation-required-message="Insira seu CPF">-->
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    @Html.TextBoxFor(model => model.cliente.Telefone, new { placeholder = "Insira seu telefone *", @class = "form-control", id = "phone", maxLength = "20" })
                                    <!--<input type="tel" class="form-control" placeholder="Insira seu telefone *" id="phone" required data-validation-required-message="Por favor, insira o telefone.">-->
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="radio">

                                    <label>@Html.RadioButtonFor(model => model.cliente.Genero, 2)Feminino</label>
                                </div>
                                <div class="radio">
                                    <label>@Html.RadioButtonFor(model => model.cliente.Genero, 1, new { @checked = true })Masculino   </label>
                                </div>
                            </div>
                            <div class="col-md-6"></div>
                            <div class="col-md-3">
                                <div class="form-group">

                                    @Html.DropDownListFor(model => model.estado, new SelectList(Model.estado, "EstadoId", "Nome", "Selecione.."), new { @class = "form-control", id = "cliente_Cidade_Estado_EstadoId" })


                                    <!--Html.TextBoxFor(model => model.cliente.Cidade.Estado.Nome, new { placeholder = "Insira seu Estado *", class = "form-control", id = "estado" })
                                    <input type="tel" class="form-control" placeholder="Estado *" id="phone" required data-validation-required-message="Por favor, insira seu telefone.">-->
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    
                                   

                                    @Html.DropDownListFor(model => model.cliente.CidadeId, new SelectList(Model.cidade, "CidadeId", "Nome", "Selecione.."), new { @class = "form-control", id = "cliente_CidadeId" })



                                   
                                    <!--Html.TextBoxFor(model => model.cliente.Cidade.Nome, new { placeholder = "Insira sua cidade *", class = "form-control", id = "cidade" })-->
                                    <!--input type="tel" class="form-control" placeholder="Insira sua cidade *" id="cidade" required data-validation-required-message="Por favor, insira o telefone.">-->
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>


                            <div class="col-md-6">
                                <div class="form-group">
                                    @Html.TextAreaFor(model => model.cliente.Mensagem, new { placeholder = "Insira sua Mensagem *", @class = "form-control", id = "message", rows = "1" })
                                    <!--<textarea class="form-control" rows="1" placeholder="Deixe uma mensagem " id="message" required data-validation-required-message="Deixe uma mensagem."></textarea>-->
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>

                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button id="btnEnviarMensagem" type="submit" class="btn btn-xl">Enviar Mensagem</button>
                            </div>
                        </div>
                    }


                </div>
            </div>
        </div>
    </section>

    <section id="investidores">
