                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    @Html.TextAreaFor(model => model.investidor.Mensagem, new { placeholder = "Insira sua Mensagem *", @class = "form-control", id = "message", rows = "1" })
                                    <!--<textarea class="form-control" rows="1" placeholder="Deixe uma mensagem " id="message" required data-validation-required-message="Deixe uma mensagem."></textarea>-->
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>

                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">Enviar Mensagem</button>
                            </div>
                        </div>
                    }

