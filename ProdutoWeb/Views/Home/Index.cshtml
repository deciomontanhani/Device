                                    @Html.TextBoxFor(model => model.investidor.Email, new { placeholder = "Seu Email *", @class = "form-control", id = "email" })
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    @Html.TextBoxFor(model => model.investidor.NomeEmpresa, new { placeholder = "Nome da sua Empresa *", @class = "form-control", id = "nomeEmpresa" })
                                    <p class="help-block text-danger"></p>
                                </div>
