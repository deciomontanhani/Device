                                    @Html.TextBoxFor(model => model.investidor.CNPJ, new { placeholder = "CNPJ da sua Empresa *", @class = "form-control", id = "cnpj" , maxLength = "25"})
                                    <p class="help-block text-danger"></p>
                                </div>
